	color: #607890;
}

.filehighlight img {
	float: left;
}

.filetitle {
	border-bottom: 2px groove #fff;
	margin-left: 55px;
	padding-bottom: 5px;
	margin-bottom: 3px;
}

.fileinfo {
	margin-left: 55px;
	overflow: hidden;
}

.fileinfo span {
	float: left;
	clear: left;
	font-size: 9px;
	padding-top: 2px;
	font-style: italic;
	font-family: Verdana;
}



/**
 * Column widths for columns in the Content section
 */

.col1_0,.col1_2,.col1_4,.col1_3,.col2_3,.col3_4 {
	float: left;
}

.col1_0 {
	width: 100%;
}

.col1_2 {
	width: 50%;
}

.col1_4 {
	width: 25%;
}

.col3_4 {
	width: 75%;
}

.col1_3 {
	width: 33%;
}

.col2_3 {
	width: 66%;
}

/*
 * Data Table Rules
 */

 div.tableframe {
     margin:2em;
 }

 table {
     font-size: .9em;
     width: 100%;
 }

 tr {
     background-color: #fff;
 }

 thead tr {
     background-color: #efefef;
 }

 th {
     text-align: left;
 }

 .listtable tr:nth-child(even), .listtable tr.even {
     background-color: #F0F9FF;
 }

 td,th {
     padding: 1em;
     border: 1px solid #ccc;
     white-space: pre-wrap;
 }

 /* 404 rules */

 body#fourohfour #content {
     background: #fff url('/core/images/404.png') no-repeat bottom right;
 }

/**
 * Footer Rules
 */

 #footer {
     border-top: 1px solid #79BA25;
     padding-top: 2em;
     clear:both;
 }



 #footer li {
 	list-style-type: none;
 	margin-left: 0.5em;
 }

 #footer .columnHeader {
 	color: #666666;
    font-family: "Franklin Gothic Book",sans-serif;
    font-size: 1.15em;
    font-weight: bold;
    padding-bottom: 0.4em;
 }

 .bf_col {
 	float: left;
 	margin-bottom: 1em;
 	width: 20%;
 }

 .contactLinks {
 	clear: left;
 	float: left;
 	display: block;
 	padding-top: 1em;
 	margin: 1em;
 	margin-left: 160px;
 }


 /* Browser hacks */
.gecko #nav {
    margin-left: 0;
}

/*
 * print styles
 * inlined to avoid required HTTP connection www.phpied.com/delay-loading-your-print-css/
 */
@media print {
  * { background: transparent !important; color: #444 !important; text-shadow: none !important; }
  a, a:visited { color: #444 !important; text-decoration: underline; }
  a:after { content: " (" attr(href) ")"; }
  abbr:after { content: " (" attr(title) ")"; }
  .ir a:after { content: ""; }  /* Don't show links for images */
  pre, blockquote { border: 1px solid #999; page-break-inside: avoid; }
  thead { display: table-header-group; } /* css-discuss.incutio.com/wiki/Printing_Tables */
  tr, img { page-break-inside: avoid; }
  @page { margin: 0.5cm; }
  p, h2, h3 { orphans: 3; widows: 3; }
  h2, h3{ page-break-after: avoid; }
}



