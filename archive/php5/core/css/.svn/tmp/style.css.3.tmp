/* Rules for status messages in content section */

.message {
    margin-bottom: 1em;
    margin-top: 1em;
}

.message p {
    margin: 0em 2em;
	padding: 1em;
	padding-left: 3em;
	background-position: 1em 1.25em;
    box-shadow: 0 0px 1px rgba(0,0,0,0.5);
}

p+.message p {
	margin-top: 0px;
}

.warning p {
	color: #da4d0a;
	background-color: #fff199;
	background-image: url(/core/images/warning_icon.png);
	background-repeat: no-repeat;
	background: url(/core/images/warning_icon.png) no-repeat 1em 1.25em, url(/core/images/warning_stripe.png) repeat top left;
}

.notice p {
	color: #006cc9;
	background-color: #d6f0ff;
	background-image: url(/core/images/info_icon.png);
	background-repeat: no-repeat;
}

.success p {
	color: #0f9400;
	background-color: #eef9e1;
	background-image: url(/core/images/check_icon.png);
	background-repeat: no-repeat;
}

.error p {
	color: #d50019;
	background-color: #ffd9da;
	background-image: url(/core/images/error_icon.png);
	background-repeat: no-repeat;
}

/* clear fix rules */
.clearfix:after,
.clearfixItems li:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

/* Breakout block rules */
h2+.breakout, h3+.breakout,h4+.breakout,h5+.breakout {
	margin-top: 1em;
}

.breakout {
	background-color: #d6f0ff;
	padding-bottom: 12px;
	margin-right: 12px;
    box-shadow: 0 1px 2px rgba(0,0,0,0.5);
}

.breakout+.breakout {
	margin-top: 2em;
}

.breakout h3 {
	margin: 0;
	padding: 0.25em 12px;
	text-align: left;
	font-size: 18px;
}

.breakout h3+p {
	padding-top: 0px;
    padding-left: 1em;
}

.breakout ul {
    margin-left: 1em;
}

.breakout ul:last-child,
.breakout li:last-child,
.breakout p:last-child {
	padding-bottom: 0;
	margin-bottom:0 !important;
}

.breakout button {
	display: block;
	margin: 0 auto;
	margin-top: 5px;
}

.fineprint {
	color: #666666;
	display: block;
    font-size: 10px;
    font-style: italic;
}

button.clean-gray {
	background: #eee;
	background: -moz-linear-gradient(top, #eee 0%, #ccc 100%);
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#eee), to(#ccc));
	border: 1px solid #ccc;
	border-bottom: 1px solid #bbb;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	color: #333;
	font-size: 12px;
	font-weight: bold;
	line-height: 1;
	padding: 8px 0;
	text-align: center;
	text-shadow: 0 1px 0 #eee;
	width: 85%;
}

button.clean-gray:hover {
  background: #ddd;
  background: -moz-linear-gradient(top, #ddd 0%, #bbb 100%);
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#ddd), to(#bbb));
  border: 1px solid #bbb;
  border-bottom: 1px solid #999;
  cursor: pointer;
  text-shadow: 0 1px 0 #ddd;
}

