/**
* layout rules
*/
body {
	margin: 0;
	padding: 0;
    background-attachment: fixed;
}

/* containers class is used to center base elements on the page that are outside the flow of the page */
.containers {
    margin: 0 auto;
    max-width: 1000px;
    min-width: 380px;
    position: relative;
    width: 100%;
}

/* Header components and blocks, menus, links, etc. */

/* Header holds all the parts of the header */
#header {
    position: fixed;
    z-index: 3;
    height: 135px;
    width: 100%;
    border-top: 62px solid #000;
}

.landing #header {
    z-index: 6;
    height: auto;
    position: relative;

}

/* navbar is the main navigation menu */

#navbar {
    position: fixed;
    top: 1.692em;
    z-index: 10;
    background: transparent url(../images/black_dusty.png) repeat top left;
    padding-right: 18px;
}

#navbar>div {
   background: #000;
}


#navbar ul {
	position: relative;
	display: block;
	list-style-type: none;
	margin: 0;
	padding: 0;
    padding:0 0 0 40px;
}

	#navbar li {
		float: left;
		list-style-type: none;
		margin: 0;
		padding: 0;

	}

		#navbar a.toplink {
            color: #dddddd;
		    display: block;
		    font-family: "Franklin Gothic Medium";
		    font-size: 13px;
		    padding: 10px 10px 0px 13px;
		    margin-top: 4px;
		    margin-left: 5px;
		    text-decoration: none;
		    height: 2em;
		    position:relative;
		    z-index: 8;
            text-shadow: 0 -1px 0 rgb(0,0,0);
		}

		#navbar li:hover a.toplink {
		    background-color: #fff;
		    color: #000;
            text-shadow:none;
		}

        #navbar li:hover .submenu,
        #headermenu li:hover .submenu {
            display:block;
        }


#navbar div#badge {
	background: url("/core/images/small_shield.png") no-repeat scroll center center transparent;
    position: absolute;
    top: 0;
    width: 40px;
    height: 40px;
}

#navbg {
    border-top: 40px solid #383838;
    height: 0;
    position: fixed;
    top: 1.692em;
    z-index: 9;
    -webkit-box-shadow: 0 3px 3px rgba(0,0,0,0.75);
    box-shadow: 0 3px 3px rgba(0,0,0,0.75);
}

/* Menu submenu rules */
.submenu {
    display:none;
    background: none repeat scroll 0 0 #fff;
    overflow: hidden;
    position: absolute;
    padding:5px 10px 10px;
    box-shadow: 2px 2px 3px rgba(0,0,0,0.75);
}

.submenu .column {
    float:left;
    width: 15.384em;
}

#navbar .submenu {
    top: 40px;
    border-left-width: 5px;
}

#navbar li:hover > a {
    color: #000;
}

 #navbar .section {
    clear: left;
}

#navbar .section ul {
    padding-left: 0;
}

#navbar .section li {
    float: none;
}

	#navbar .section li a {
	    max-width: none;
	    width: auto;
	    text-decoration: none;
	    padding: 5px 5px;
	    display:block;
	    line-height: 1;
	    height: 1em;
	    background: transparent;
	    color: #000;
	    margin-left:0;
	}


#navbar .pointer {
    position: relative;
    right: -10px;
    margin-top: -21px;
    border-top: 4px solid transparent;
    border-right: 0px;
    border-left: 5px solid #fff;
    border-bottom: 4px solid transparent;
    display: block;
    height: 0;
    width: 0;
    z-index: 8;
}

#navbar li:hover .pointer {
    position: relative;
    margin-top: -21px;

    border-top: 6px solid #000;
    border-bottom: 0px;
    border-left: 3px solid transparent;
    border-right: 3px solid transparent;
    display: block;
    height: 0;
    width: 0;
}

#headermenu .submenu li {
    list-style-type: none;
    padding: 0;
    margin:0;
}

#headermenu .submenu a {
    text-decoration: none;
    padding: 5px 5px;
    display:block;
}

#headermenu li:hover .pointer {
    border-bottom: 0px;
    border-left: 3px solid #fff;
    border-right: 3px solid #fff;
    border-top: 5px solid #000;
    top: 11px;
    margin-left: 8px;
    z-index: 8;
}

/* headerbox holds the title info on interior pages */
#headerbox {
    background: url("/core/images/woodgrain7.jpg") no-repeat scroll 0 0 transparent;
    height: 100px;
    position: relative;
    z-index: 3;
    box-shadow: inset 0 -2px 4px rgba(0,0,0,0.75);
}

/* headerbox is shrunk down on portal pages */
.landing #headerbox {
    height: 360px;
}

/* headermenu holds the secondary menu */
#headermenu {
    z-index: 11;
    position: fixed;
    top: 0;
    background: #ccc url(../images/noise_light.jpg) repeat-x top left;
    border-bottom:2px groove #fff;
}

#headermenu ul {
    margin: 0;
    padding: 0;
    overflow: hidden;

}

#headermenu>ul {
    width: 446px;
    float: right;
    padding: 3px 0;
}

	#headermenu>ul>li {
	    list-style-type: none;
	    float: right;
	    margin: 0;
	    padding: 0;
	}

		#headermenu>ul>li>a {
		    color: #272727;
		    text-decoration: none;
		    text-shadow: 0 1px 0 #fff;
		    padding: .5em;
		    padding-left: 14px;
		    padding: 3px 18px;
		    font-family: "Franklin Gothic Medium", Helvetica, Arial, Verdana, sans-serif;
		    position: relative;
		    z-index: 8;
		}

		#headermenu li:hover > a:last-child {
		    background: #f7f7f7;
		}

		#headermenu li:hover > a {
		    background: #f7f7f7;

		}

#headermenu .pointer {
    display: block;
    height: 0px;
    width: 0px;
    border-top: 3px solid transparent;
	border-right: 0px;
	border-left: 5px solid #000;
	border-bottom: 3px solid transparent;
    margin-left: 9px;
    position: absolute;
    top: 9px;
}

#navbar .section a,
#headermenu .section a{
    text-align: left;
    font-size: .9em;
    font-family: "Franklin Gothic Book", Helvetica, Arial, Verdana, sans-serif;
    font-weight: normal;
    color: #000;
    border: 0;
    white-space:normal;
}


#navbar .section a:hover,
#headermenu .section a:hover{
    font-family: "Franklin Gothic Book", Helvetica, Arial, Verdana, sans-serif;
    font-style: normal;
    background-color: #3B546F;
    color:#fff;
    border-radius: 3px;
}

/* Column rules */

.submenu .column {
    float:left;
    width: 14.384em;
}


.submenu .column .section {
    margin: 0 0.5em;
}

.submenu .column>.section h3 {
